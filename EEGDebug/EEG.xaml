        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="9*" />
                <ColumnDefinition Width="0*" />
            </Grid.ColumnDefinitions>
            <lvc:CartesianChart Grid.Column="0" Series="{Binding CHOne}" DisableAnimations="True" Hoverable="False" DataTooltip="{x:Null}" Height="80" >
                <lvc:CartesianChart.AxisX>
                    <lvc:Axis Labels="{Binding Labels}"></lvc:Axis>
                </lvc:CartesianChart.AxisX>
                <lvc:CartesianChart.AxisY>
                    <lvc:Axis Labels="{Binding Labels}"></lvc:Axis>
                </lvc:CartesianChart.AxisY>
            </lvc:CartesianChart>
            <TextBox x:Name="textBox_ch1" FontWeight="Bold" HorizontalAlignment="Left" Grid.Column="1" Width="100" IsReadOnly="True"/>
        </Grid>
